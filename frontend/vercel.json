{
     "routes": [{ "src": "/[^.]+", "dest": "/", "status": 200 }] ,

     "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
     "headers": [
        {
          "source": "/(.*).(html|js|css)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "no-store"
            }
          ]
        }
      ]
}